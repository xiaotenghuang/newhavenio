    "@storybook/addon-links": "^5.2.3",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "ava": "^2.4.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "chrome-launcher": "^0.12.0",
    "core-js": "^3.2.1",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
